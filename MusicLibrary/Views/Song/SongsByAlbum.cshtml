@model IEnumerable<Song>
@foreach (var s in Model)
{
<div>


        <h3>@s.Title</h3>
        <h4>@s.Artist</h4>

        @foreach (SongAlbum a in s.SongAlbums)
        {
            <h4>@a.Album.Name</h4>
        }
</div>


}